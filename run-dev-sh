#!/bin/bash

BASE_DIR="$(dirname "$(readlink -f "${BASH_SOURCE}")")"

source ${BASE_DIR}/build-dev

if [[ $(docker container ls | grep "${CONTAINER_DEV_APP}" | wc -l) == '0' ]]; then
    echo 'App container is not running, first run ./run-dev-app'
else
    docker exec -it "${CONTAINER_DEV_APP}" bash
fi
